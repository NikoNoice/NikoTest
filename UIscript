local function YSMB_fake_script() -- ShadowsB.LocalScript 
	local script = Instance.new('LocalScript', ShadowsB)

	
	
	toggle = false
	script.Parent.MouseButton1Down:connect(function()
	
		if toggle == true then 
			toggle = false 
		else
			toggle = true
		end
		if toggle == true then 
			script.Parent.BackgroundColor3 = Color3.fromRGB(154, 0, 77)
		end
		if toggle == false then 
			script.Parent.BackgroundColor3 = Color3.fromRGB(48, 48, 48)
		end
	
		if toggle == false then
			game.Lighting.GlobalShadows = false
			print("off")
		end
		if toggle == true then
			game.Lighting.GlobalShadows = true
			print("on")
		end
	end)
end

local function VFHS_fake_script() -- GunButton.LocalScript 
	local script = Instance.new('LocalScript', GunButton)

	-- Gun Button
	
	local button = script.Parent
	
	local function onButtonActivated()
		print("Button activated!")
		script.Parent.BorderSizePixel = 1
		script.Parent.TextColor3 = Color3.fromRGB(154, 0, 77)
		script.Parent.Framegun.Visible = true
		script.Parent.Parent.VisualsButton.BorderSizePixel = 0
		script.Parent.Parent.MainButton.BorderSizePixel = 0
		script.Parent.Parent.MainButton.Framemain.Visible = false
		script.Parent.Parent.VisualsButton.Framevis.Visible = false
		script.Parent.Parent.VisualsButton.TextColor3 = Color3.fromRGB(255, 255, 255)
		script.Parent.Parent.MainButton.TextColor3 = Color3.fromRGB(255, 255, 255)
	
	end
	
	button.Activated:Connect(onButtonActivated)
end
